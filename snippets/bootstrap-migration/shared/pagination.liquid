<nav aria-label="Page navigation">
    <ul class="pagination">
        {% if paginator.previous %}
            <li class="page-item">
                <a class="page-link"
                    href="{{ paginator.previous.url }}"
                    title="{{ paginator.previous.title }}">
        {% else %}
            <li class="page-item disabled">
                <a class="page-link"
                    title="{{ paginator.previous.title }}">
        {% endif %}
                <span aria-hidden="true">&laquo;</span>
            </a>
        </li>

        {% for part in paginator.parts %}
            {% if part.is_link %}
                <li class="page-item">
                    <a class="page-link"
                        href="{{ part.url }}">
                        {{ part.title }}
                    </a>
                </li>
            {% else %}
                {% if part.title == paginator.current_page %}
                    <li class="page-item active">
                        <a class="page-link">{{ part.title }}</a>
                    </li>
                {% else %}
                    <li class="page-item">
                        <a class="page-link">{{ part.title }}</a>
                    </li>
                {% endif %}
            {% endif %}
        {% endfor %}

        {% if paginator.next %}
            <li class="page-item">
                <a class="page-link"
                    href="{{ paginator.next.url }}"
                    title="{{ paginator.next.title }}">
        {% else %}
            <li class="page-item disabled">
                <a class="page-link"
                    title="{{ paginator.next.title }}">
        {% endif %}
                <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
    </ul>
</nav>
