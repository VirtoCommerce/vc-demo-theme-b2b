{% assign index_mod = for.index | modulo: column_count %}
{% assign last_column_index = column_count | minus: 1 %}
{% assign should_start_row = false %}
{% assign should_end_row = false %}
{% if index_mod == 0 %}
    {% assign should_start_row = true %}
{% endif %}
{% if index_mod == last_column_index or for.last %}
    {% assign should_end_row = true %}
{% endif %}

